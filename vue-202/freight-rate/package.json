{
  "name": "freight-rate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "vite",
    "kill": "lsof -t -i:3003 | xargs kill -9",
    "preview": "vite preview",
    "serve:test": "python3 $PWD/server/test.py",
    "serve:watch": "cd $PWD/server && python3 -m flask --app ./app.py --debug run",
    "serve": "cd $PWD/server && python3 -m waitress --port=3003 --call 'app:create_app'",
    "test": "vitest",
    "z": "cd $PWD/server && python3 app.py"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.0.0",
    "@tsconfig/svelte": "^3.0.0",
    "svelte": "^3.54.0",
    "svelte-check": "^2.10.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "dependencies": {
    "vitest": "^0.28.1"
  }
}
